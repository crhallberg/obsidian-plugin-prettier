{
  "name": "obsidian-plugin-prettier",
  "description": "Opinionated formatting for your obsidian.md notes.",
  "main": "main.js",
  "scripts": {
    "start": "obsidian-plugin dev --esbuild-override=esbuild.config.json src/main.ts",
    "build": "obsidian-plugin build --esbuild-override=esbuild.config.json src/main.ts",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "release": "auto shipit"
  },
  "keywords": [
    "obsidian.md",
    "plugin"
  ],
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "repository": "hipstersmoothie/",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/prettier": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "auto": "^10.16.7",
    "auto-plugin-obsidian": "^0.1.4",
    "eslint": "7.12.1",
    "eslint-config-prettier": "^7.2.0",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/archive/7f3927722e1bdd791119727236a12bb10763dd04.tar.gz",
    "obsidian-plugin-cli": "^0.4.0",
    "prettier": "^2.2.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
