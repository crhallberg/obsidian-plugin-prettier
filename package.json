{
  "name": "obsidian-plugin-prettier",
  "version": "0.11.0",
  "description": "Opinionated formatting for your obsidian.md notes.",
  "main": "main.js",
  "scripts": {
    "start": "yarn build -w",
    "build": "mkdir -p dist && cp manifest.json dist && rollup --config rollup.config.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "release": "auto shipit"
  },
  "keywords": [
    "obsidian.md",
    "plugin"
  ],
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "repository": "hipstersmoothie/",
  "license": "MIT",
  "devDependencies": {
    "@auto-it/git-tag": "^10.16.7",
    "@auto-it/upload-assets": "^10.16.7",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/node": "^14.14.2",
    "@types/prettier": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "auto": "^10.16.7",
    "eslint": "7.12.1",
    "eslint-config-prettier": "^7.2.0",
    "obsidian": "https://github.com/obsidianmd/obsidian-api/archive/7f3927722e1bdd791119727236a12bb10763dd04.tar.gz",
    "prettier": "^2.2.1",
    "rollup": "^2.32.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "auto": {
    "plugins": [
      "git-tag",
      "released",
      [
        "upload-assets",
        [
          "./prettier-format.zip"
        ]
      ]
    ]
  }
}
